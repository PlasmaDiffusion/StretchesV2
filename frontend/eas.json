{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_VARIANT": "dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_VARIANT": "dev"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "channel": "production",
      "env": {
        "APP_VARIANT": "prod"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
